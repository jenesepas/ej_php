<?php
        


//Creamos un cliente para llamar a esa URL. 
$discos = new SoapClient("http://localhost/php/t06/sw_server/servicio.php?wsdl");


// Obtenemos el nombre del musico con cod=30
$nombre_musico = $discos->Dame_Nombre_Musico(25);
print("El nombre del musico 25 es: ".$nombre_musico);
print "<br />";

//obtenemos el grupo y la fecha fundacion posterior a la fecha pasada.
print($discos->Dame_Grupo_Viejo("Reino Unido","1971-01-01").", posterior a la fecha: 1971-01-01 y de Reino Unido.");
        
        
?>